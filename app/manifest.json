{
  "minimum_chrome_version": "25",
  "manifest_version": 2,
  "name": "Beagle Term (Beta)",
  "version": "1.7.0",
  "icons": {
    "128": "images/beagleterm-128.png",
    "48": "images/beagleterm-48.png",
    "32": "images/beagleterm-32.png",
    "16": "images/beagleterm-16.png"
  },
  "description": "Beagle Term is serial terminal emulator.",
  "offline_enabled": true,
  "permissions": [
    "serial",
    "storage",
    "fileSystem"
  ],
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  }
}
